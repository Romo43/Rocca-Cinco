<div class="carousel-inner" role="listbox" style=" width:100%; height: 600px;">

    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100"
                    src="https://scontent.fcjs3-1.fna.fbcdn.net/v/t1.6435-9/186510042_759488884727583_6526295004847731873_n.jpg?_nc_cat=102&ccb=1-3&_nc_sid=730e14&_nc_ohc=DSk3KYYbx_AAX8t63G0&tn=i5nJfSf3VvY1RciX&_nc_ht=scontent.fcjs3-1.fna&oh=42fe83cfc6c580791a19e5c60cfba34b&oe=6122EF1F"
                    alt="First slide" style="object-fit:cover; width:100%; height: 600px;">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100"
                    src="https://scontent.fcjs3-1.fna.fbcdn.net/v/t1.6435-9/185739413_759488944727577_7593583766649251394_n.jpg?_nc_cat=107&ccb=1-3&_nc_sid=730e14&_nc_ohc=1tRLL1VsTfEAX-lWNCD&_nc_oc=AQnfJyVa8BTmnbKjeQzhrJbgUuYjFV-FppmYbCS951YIi7jVjTvMLVV6bWCq55mt5i7Ow4OVgX3vDcfniz5V06wY&_nc_ht=scontent.fcjs3-1.fna&oh=de7c14458c2d5de59f0d5332fdeed2fb&oe=6121224D"
                    alt="Second slide" style="object-fit:cover; width:100%; height: 600px;">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100"
                    src="https://scontent.fcjs3-1.fna.fbcdn.net/v/t1.6435-9/186229525_759488914727580_213834493239735506_n.jpg?_nc_cat=102&ccb=1-3&_nc_sid=730e14&_nc_ohc=HPh-LoMX9koAX9dWxG6&_nc_ht=scontent.fcjs3-1.fna&oh=915d237457a9badf11787e53e8c60926&oe=6123239E"
                    alt="Third slide" style="object-fit:cover; width:100%; height: 600px;">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

<div>
    {{#each data}}
    <div class="big">
        <article class="recipe">
            <div class="recipe-img">
                <img src="{{URLimage}}" width="1500" height="1368" alt="">
            </div>
            <div class="recipe-content">
                <h5 class="recipe-title">{{name}}</h5>
                <p class="recipe-desc">
                    {{description}}
                </p>
                <p class="recipe-price">{{price}}</p>
            </div>
        </article>
    </div>
    {{/each}}
</div>